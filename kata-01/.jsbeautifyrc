{
	"html": {
		"indent_char": "\t",
		"indent_size": 1,
		"unformatted": ["a", "sub", "sup", "b", "i", "u", "pre"],
		"wrap_line_length": 0,
		"preserve_newlines": true,
		"end_with_newline": true
	},
	"css": {
		"allowed_file_extensions": ["css", "scss"],
		"end_with_newline": false,
		"indent_char": "\t",
		"indent_size": 1,
		"selector_separator": " ",
		"selector_separator_newline": false
	},
	"js": {
		"indent_with_tabs": true,
		"indent_size": 1,
		"indent_char": "\t"
	}
}
